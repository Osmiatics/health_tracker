<p id="notice"><%= notice %></p>

<h1>Listing Kilocal Counts</h1>

<table>
  <thead>
    <tr>
      <th>Daily goal</th>
      <th>Daily intake</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @kilocals.each do |kilocal| %>
      <tr>
        <td><%= kilocal.daily_goal %></td>
        <td><%= kilocal.daily_intake %></td>
        <td><%= kilocal.date %></td>
        <td><%= link_to 'Show', kilocal %></td>
        <td><%= link_to 'Edit', edit_kilocal_path(kilocal) %></td>
        <td><%= link_to 'Destroy', kilocal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Kilocal', new_kilocal_path %>
<%= link_to 'Dashboard', root_path %>
